{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "orgncf",
      "camelcase": "OrgNCf",
      "title": "Organizator Network Configuration",
      "scope": "source.orgncf",
      "file-types": [
        "ncf"
      ],
      "injection-regex": "^orgncf$",
      "class-name": "TreeSitterOrgncf"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "AGPL3",
    "description": "Describes into a single file how lan items should be added to DNS and DHCP",
    "authors": [
      {
        "name": "Ovidiu Ionescu",
        "email": "ovidiu@ionescu.net",
        "url": "https://github.com/ovidiu-ionescu"
      }
    ],
    "links": {
      "repository": "https://github.com/ovidiu-ionescu/tree-sitter-orgncf"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}